{% extends "../tmpl/layout.twig" %}

{% block head %}
{% endblock %}

{% block body %}
    <div class="container">
        <div class="jumbotron">
            <h1>{{ title }}</h1>
            <h2>Submitted by {{ user }} {% if isCurrentUser%} (you) {% endif %}</h2>
            <h3>tags: {{ tags }}</h3>
            <h4>desc: {{ desc }}</h4>
            <img src="{{ url }}" width="35%">
        </div>
    </div>

{% endblock %}